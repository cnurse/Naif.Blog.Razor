    <PropertyGroup>
        <TargetFramework>net5.0</TargetFramework>
        <AddRazorSupportForMvc>true</AddRazorSupportForMvc>
        <ProductVersion>1.2.0</ProductVersion>
        <FileVersion>1.2.0.14</FileVersion>
        <NuspecFile>$(MSBuildProjectName).nuspec</NuspecFile>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Microsoft.AspNetCore.Components.Authorization" Version="5.0.10" />
      <PackageReference Include="Naif.Auth0" Version="2.1.0-beta.7" />
      <PackageReference Include="Naif.Blog.Core" Version="3.1.0-beta.2" />
      <PackageReference Include="Naif.Core" Version="3.3.0-beta.4" />
    </ItemGroup>

</Project>
